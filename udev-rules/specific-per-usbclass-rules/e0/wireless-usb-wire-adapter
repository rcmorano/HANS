
ACTION=="add", SUBSYSTEM=="usb", \
	ATTRS{bDeviceClass}=="e0", \
        ATTRS{bDeviceSubClass}=="02", \
        ATTRS{bDeviceProtocol}=="03", \
	ENV{MIMETYPE}="x-usb-device/wireless-usb-wire-adapter", \
        ENV{HANS_TAG}!="1", \
	ENV{HANS_TAG}="1", \
	RUN+="/usr/bin/hans $env{MIMETYPE} $devpath"

