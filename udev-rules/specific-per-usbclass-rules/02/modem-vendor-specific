
ACTION=="add", SUBSYSTEM=="usb", \
	ATTRS{bDeviceClass}=="02", \
        ATTRS{bDeviceSubClass}=="02", \
        ATTRS{bDeviceProtocol}=="ff", \
	ENV{MIMETYPE}="x-usb-device/modem-vendor-specific", \
        ENV{HANS_TAG}!="1", \
	ENV{HANS_TAG}="1", \
	RUN+="/usr/bin/hans $env{MIMETYPE} $devpath"

