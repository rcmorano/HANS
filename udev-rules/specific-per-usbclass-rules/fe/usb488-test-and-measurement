
ACTION=="add", SUBSYSTEM=="usb", \
	ATTRS{bDeviceClass}=="fe", \
        ATTRS{bDeviceSubClass}=="03", \
        ATTRS{bDeviceProtocol}=="02", \
	ENV{MIMETYPE}="x-usb-device/usb488-test-and-measurement", \
        ENV{HANS_TAG}!="1", \
	ENV{HANS_TAG}="1", \
	RUN+="/usr/bin/hans $env{MIMETYPE} $devpath"

